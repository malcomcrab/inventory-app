    <!DOCTYPE html>
    <html lang="en">
    <head>
        <meta charset="UTF-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <link rel="stylesheet" href="/styles.css">
        <title><%= title %>></title>
    </head>

    <body>
        <h1><%= title %></h1>

        <%- include('navbar', {links: links}) %>
        <ul>
            <% characters.forEach(character => {%>
                <li> <p>Name: <%= character.first_name%> <%= character.last_name %>, First Appearence: <%= character.first_appearence %>, Hometown: <%= character.hometown %>, Faction: <%= character.faction %></p>
                    <a href="<%= character.id %>/update-form">Edit</a>
                    <form action="/<%= character.id %>/delete" method="POST" style="display:inline;">
                        <button type="submit" onclick="return confirm('Are you sure you want to delete this user?');">Delete</button>
                      </form>
                </li>
            <% }); %>

        </ul>
    </body>


    </html>

   